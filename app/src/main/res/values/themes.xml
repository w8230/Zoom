<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Zoom" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <!-- colorPrimary : 대표 색상 -->
        <!-- colorPrimaryDark : 상태바 색상 -->
        <!-- colorAccent : 대조/대비 색상 (버튼색) -->
        <item name="colorPrimary">@color/theme_6db645</item>
        <item name="colorPrimaryVariant">@color/_yellow</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/theme_294929</item>

        <!-- Customize your theme here. -->
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
        <item name="colorAccent">@color/_yellow</item>
    </style>
</resources>